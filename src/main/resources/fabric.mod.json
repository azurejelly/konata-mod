{
	"schemaVersion": 1,
	"id": "konata",
	"version": "${version}",
	"name": "Konata Mod",
	"description": "Adds some Konata Izumi images or jumpscares to your Minecraft client.",
	"authors": [
		"azurejelly"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/konata/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"dev.azuuure.konata.KonataMod"
		],
		"modmenu": [
			"dev.azuuure.konata.compat.ModMenuApiImpl"
		]
	},
	"mixins": [
		"konata.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.7",
		"java": ">=21",
		"fabric-api": "*",
		"modmenu": "*"
	}
}